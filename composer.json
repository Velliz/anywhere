{
  "name": "velliz/anywhere",
  "description": "Pukoframework based Web Console that simplify data object transition state",
  "type": "web console",
  "license": "MIT",
  "version": "1.1.6",
  "authors": [
    {
      "name": "Didit Velliz",
      "email": "diditvelliz@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "puko/framework": "dev-master",
    "dompdf/dompdf": "v0.8.1",
    "aferrandini/phpqrcode": "1.0.1",
    "phpmailer/phpmailer": "~6.0",
    "phpoffice/phpspreadsheet": "dev-master",
    "puko/console": "dev-master",
    "erusev/parsedown": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.13"
  },
  "autoload": {
    "psr-4": {
      "controller\\": "controller/",
      "model\\": "model/",
      "plugins\\": "plugins/",
      "tests\\": "tests/"
    }
  }
}
