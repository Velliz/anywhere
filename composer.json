{
  "name": "velliz/anywhere",
  "description": "Pukoframework based Web Console that simplify data object transition state",
  "license": "MIT",
  "version": "1.1.6",
  "authors": [
    {
      "name": "Didit Velliz",
      "email": "diditvelliz@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1|>=8.0",
    "puko/framework": "^1.3.3",
    "vlucas/phpdotenv": "5.5.x-dev",
    "dompdf/dompdf": "v0.8.5",
    "endroid/qr-code": "4.3.0",
    "phpmailer/phpmailer": "v6.6.4",
    "phpoffice/phpspreadsheet": "1.20.0",
    "phpoffice/phpword": "0.18.3",
    "erusev/parsedown": "dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.0",
    "puko/console": "^0.3.3"
  },
  "autoload": {
    "psr-4": {
      "controller\\": "controller/",
      "model\\": "model/",
      "plugins\\": "plugins/",
      "tests\\": "tests/"
    }
  }
}
